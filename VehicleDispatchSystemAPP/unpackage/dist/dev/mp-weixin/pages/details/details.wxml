<view class="content"><view class="card-info"><view class="card-info-nav"><label class="title _span">{{area+'校区'}}</label><label class="tl _span">|</label><label class="title _span">选择车辆</label></view><view class="card-info-time"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="start-time" bindtap="__e"><view class="fl"><view class="specific-time fl"><label class="_span">{{startTime.weekday}}</label><label class="_span">{{startTime.hours+":"+startTime.minutes}}</label></view><view class="about-time"><label class="_span">{{startTime.month}}</label>月<label class="_span">{{startTime.day}}</label>日</view></view></view><view class="day-time"><image src="../../static/index/line.png"></image><label class="_span">{{day+"天"}}</label></view><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="end-time" bindtap="__e"><view><view class="specific-time fr"><label class="_span">{{endTime.weekday}}</label><label class="_span">{{endTime.hours+":"+endTime.minutes}}</label></view><view class="about-time"><label class="_span">{{endTime.month}}</label>月<label class="_span">{{endTime.day}}</label>日</view></view></view></view><view style="margin-top:150rpx;"></view></view><view class="card-list"><view class="options"><view data-event-opts="{{[['tap',[['optionSort',['$event']]]]]}}" class="option" bindtap="__e"><label class="_span">排序</label><image src="{{arrows[sortIdx]}}"></image></view><view data-event-opts="{{[['tap',[['optionSelect',['$event']]]]]}}" class="option" bindtap="__e"><label class="_span">筛选</label><image src="{{arrows[selectIdx]}}"></image></view></view><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="key"><view class="card-item"><image class="show" src="{{baseURL+item.$orig.modelImg}}"></image><view class="text"><view class="title _p">{{item.$orig.name}}</view><label class="type _span">{{item.$orig.type+"/"+item.$orig.driverType+"/"+item.$orig.displacement+"/"+item.$orig.enerty}}</label><label class="type _span">{{item.$orig.color+"/"+item.$orig.year+"/"}}<label class="_span">{{''+item.$orig.carNumber[0]+item.$orig.carNumber[1]+"*****"}}</label><block wx:if="{{item.g0==8}}"><label class="_span">*</label></block></label></view><view class="bottom"><view class="bottom-label fl"><block wx:if="{{item.$orig.noDeposit==true}}"><view class="label vip"><image src="../../static/details/vip.png"></image><label class="_span">会员免押</label></view></block><block wx:if="{{item.$orig.luxury==true}}"><view class="label car-luxury"><image src="../../static/details/luxury.png"></image><label class="_span">豪华品牌</label></view></block><block wx:if="{{item.$orig.newEnergy==true}}"><view class="label car-green"><image src="../../static/details/carNumber.png"></image><label class="_span">新能源</label></view></block></view><view class="bottom-label fr"><view class="label price fr"><image src="../../static/details/price.png"></image><label class="_span">预计费用</label><label class="_span">{{"￥"+item.$orig.price*day}}</label></view></view></view></view></block></view><view class="on-reach-bottom">{{reachTip}}</view><view hidden="{{!(sortIdx==1)}}" class="option-sort"><label data-event-opts="{{[['tap',[['sortPrice',[1]]]]]}}" class="{{['_span',price==1?'check':'']}}" bindtap="__e">优先低租金</label><label data-event-opts="{{[['tap',[['sortPrice',[2]]]]]}}" class="{{['_span',price==2?'check':'']}}" bindtap="__e">优先高租金</label></view><view hidden="{{!(selectIdx==1)}}" class="option-select"><view><view><label class="_span">类型</label></view><label data-event-opts="{{[['tap',[['selectType',[0]]]]]}}" class="{{['_span',typeAll==true?'check':'']}}" bindtap="__e">全部</label><block wx:for="{{types}}" wx:for-item="item" wx:for-index="idx"><label data-event-opts="{{[['tap',[['selectType',[idx+1]]]]]}}" class="{{['_span',typeBools[idx]==true?'check':'']}}" bindtap="__e">{{item}}</label></block></view><view><view><label class="_span">价格 (按日租)</label></view><label data-event-opts="{{[['tap',[['selectPrice',[0]]]]]}}" class="{{['_span',priceAll==true?'check':'']}}" bindtap="__e">全部</label><block wx:for="{{prices}}" wx:for-item="item" wx:for-index="idx"><label data-event-opts="{{[['tap',[['selectPrice',[idx+1]]]]]}}" class="{{['_span',priceBools[idx]==true?'check':'']}}" bindtap="__e">{{item}}</label></block></view><view><view><label class="_span">标签</label></view><label data-event-opts="{{[['tap',[['selectLabel',[0]]]]]}}" class="{{['_span',labelAll==true?'check':'']}}" bindtap="__e">全部</label><block wx:for="{{labels}}" wx:for-item="item" wx:for-index="idx"><label data-event-opts="{{[['tap',[['selectLabel',[idx+1]]]]]}}" class="{{['_span',labelBools[idx]==true?'check':'']}}" bindtap="__e">{{item}}</label></block></view><view></view></view></view>